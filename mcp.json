{
  "mcpServers": {
    "wikipedia-dotnet": {
      "command": "dotnet",
      "args": [
        "run",
        "--project",
        "/full/path/to/your/WikipediaMcpServer/src/WikipediaMcpServer/WikipediaMcpServer.csproj",
        "--",
        "--mcp"
      ],
      "env": {
        "DOTNET_ENVIRONMENT": "Production"
      }
    }
  }
}